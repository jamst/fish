<%= render layout: 'shards/default_box', locals: {} do |box| %>
    <div class="col-md-12">
      <% case box when Box::Body %>
        <div class="row">
          <%= render '/shards/form_template' %>
        </div>
          <div class="row">
            <table id="alive_report_list" class="table table-bordered table-hover"  style="table-layout:fixed;" cellspacing="0" width="100%">
              <thead>
                <tr>
                  <th tag="report_time" >日期</th>
                  <th tag="tag" >标签</th>
                  <th tag="activation_num"  field_exec="delimiter">新增</th>
                  <th tag="stay_alive2"  field_exec="delimiter">2日</th>
                  <th tag="ratio_alive2" field_exec="percentage"></th>
                  <th tag="stay_alive3"  field_exec="delimiter">3日</th>
                  <th tag="ratio_alive3" field_exec="percentage"></th>
                  <th tag="stay_alive7"  field_exec="delimiter">7日</th>
                  <th tag="ratio_alive7" field_exec="percentage" ></th>
                  <th tag="stay_alive15"  field_exec="delimiter">15日</th>
                  <th tag="ratio_alive15" field_exec="percentage"  ></th>
                  <th tag="stay_alive30"  field_exec="delimiter">30日</th>
                  <th tag="ratio_alive30" field_exec="percentage"  ></th>
                </tr>
              </thead>
              <tbody>

              </tbody>

              <tfoot class="well">
                <tr index="0">
                  <td>中位数</td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td tag="median_ratio2" field_exec="percentage"></td>
                  <td></td>
                  <td tag="median_ratio3" field_exec="percentage"></td>
                  <td></td>
                  <td tag="median_ratio7" field_exec="percentage"></td>
                  <td></td>
                  <td tag="median_ratio15" field_exec="percentage"></td>
                  <td></td>
                  <td tag="median_ratio30" field_exec="percentage"></td>
                </tr>
                <tr index="1">
                  <td>平均留存率</td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td tag="stay_aliva_ratio2" field_exec="percentage"></td>
                  <td></td>
                  <td tag="stay_aliva_ratio3" field_exec="percentage"></td>
                  <td></td>
                  <td tag="stay_aliva_ratio7" field_exec="percentage"></td>
                  <td></td>
                  <td tag="stay_aliva_ratio15" field_exec="percentage"></td>
                  <td></td>
                  <td tag="stay_aliva_ratio30" field_exec="percentage"></td>
                </tr>
              </tfoot>
            </table>
          </div>
      <% end %>
    </div>
<% end %>
